import java.util.Scanner;


public class Unique {



        public static String Uni(String str) {
            String result = "";
            int i = 0;
            char letter;
            while (i < str.length()) {
                letter=str.charAt(i);
                if(str.lastIndexOf(letter)==i && str.indexOf(letter)==i)
                    result+=letter;
                i++;
            }
            return result;
        }

    public static void main(String[] args) {
            Scanner scan = new Scanner(System.in);
                String string = scan.nextLine();
                System.out.println(Uni(string));

    }
}
