import java.util.Arrays;
import java.util.Scanner;

public class Mass {

        static int getLength(int[] arr){
            int leng = 0;
            for(int i=0; i< arr.length; i++){
                if(arr[i]<0){
                    leng++;
                }
            }
            return leng;
        }
        static int[] getNegativeArr(int[] arr){
            int[] negativeArr = new int[getLength(arr)];
            for (int i = 0, a=0; i<arr.length; i++){
                if (arr[i]<0){
                    negativeArr[a] = arr[i];
                    a++;
                }
            }
            return negativeArr;

        }
        static int[] getPositive(int[] arr){
            int leng = arr.length - getLength(arr);
            int[] positiveArr = new int[leng];
            for(int i=0, a=0; i<arr.length; i++){
                if (arr[i]>= 0){
                    positiveArr[a] = arr[i];
                    a++;
                }
            }
            return positiveArr;
        }

    public static void main(String[] args) {
            Scanner scan = new Scanner(System.in);
            int size = scan.nextInt();
            int[] result = new int[size];
            for (int i=0; i< size; i++){
                result [i] = scan.nextInt();
            }

        System.out.println(Arrays.toString(getNegativeArr(result)));
        System.out.println(Arrays.toString(getPositive(result)));
    }
}
